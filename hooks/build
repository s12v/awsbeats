#!/bin/sh
docker build --build-arg AWSBEATS_VERSION="${AWSBEATS_VERSION}" --build-arg GO_VERSION="${GO_VERSION}" --build-arg GO_PLATFORM="linux-amd64" --build-arg BEAT_GITHUB_REPO="${BEAT_GITHUB_REPO}" --build-arg BEAT_GO_PKG="${BEAT_GO_PKG}" --build-arg BEAT_DOCKER_IMAGE="${BEAT_DOCKER_IMAGE}" --build-arg BEATS_VERSION="${BEATS_VERSION}" --build-arg BEAT_NAME="${BEAT_NAME}" -t "${DOCKER_REPO}:${DOCKER_TAG}" .
